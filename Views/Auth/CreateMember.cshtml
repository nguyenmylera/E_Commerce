@model WebApp.Models.Member

<h2>Thêm thành viên</h2>

<form asp-action="CreateMember" method="post">
    <div>
        <label>Email:</label>
        <input asp-for="Email" required />
    </div>
    <div>
        <label>Password:</label>
        <input asp-for="Password" type="password" required />
    </div>
    <div>
        <label>Name:</label>
        <input asp-for="Name" required />
    </div>
    <div>
        <label>Given Name:</label>
        <input asp-for="GivenName" required />
    </div>
    <div>
        <label>Surname:</label>
        <input asp-for="Surname" required />
    </div>
    <button type="submit">Thêm</button>
</form>
